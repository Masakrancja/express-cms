extends layout

block content
  h1= title

  if message !== ''
    p=message 

  form(method='GET')
    input(type='text' name='search' value=search)
    input(type='submit' value="Szukaj")

  each item in data
    h2(class='title')=item.title 
    p(class='data')=item.created 
    p=item.description
